class Alumno:
    nombre = None
    nota = None

    def cambiarNombre(self):
        self.nombre = "Rubén"
    
    def cambiarNota(self):
        self.nota = 7

alumno = Alumno()

print(alumno.nombre, alumno.nota)

alumno.cambiarNombre()
alumno.cambiarNota()

if(alumno.nota >= 5):
    print(alumno.nombre, "está aprobado")
else:
    print(alumno.nombre, "está suspendido")
